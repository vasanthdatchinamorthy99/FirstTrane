		},
		{
			"DataType": "Button",
			"CommonXpath": "//button[text()='<fieldLabel>']"
		},
		{
			"DataType": "Look Up",
			"CommonXpath": "//label[text()='<fieldLabel>']//following-sibling::div//input"
		},
		{
			"DataType": "Look Up Value",
			"CommonXpath": "//lightning-base-combobox-formatted-text[@title= '<fieldLabel>']"
